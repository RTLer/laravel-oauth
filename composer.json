{
    "name": "rtler/laravel-oauth2",
    "description": "thephpleague oauth2 integration for laravel and mongo",
    "license": "MIT",
    "type": "project",
    "require": {
        "league/oauth2-server": "5.*",
        "jenssegers/mongodb": "3.*",
        "guzzlehttp/psr7": "^1.3",
        "symfony/psr-http-message-bridge": "^0.2.0",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "orchestra/testbench": "~3.2"
    },
    "authors": [
        {
            "name": "hooman naghiee",
            "email": "hooman@RTLer.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "RTLer\\Oauth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oauth2Tests\\": "tests/"
        }
    }
}
